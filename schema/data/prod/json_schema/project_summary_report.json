{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Project Summary Report",
    "required": [
      "reportDueDate",
      "projectSummaryReportPayment",
      "dateSentToCsnr"
    ],
    "properties": {
      "reportDueDate": {
        "type": "string",
        "title": "Report Due Date",
        "default": "undefined"
      },
      "submittedDate": {
        "type": "string",
        "title": "Received Date",
        "default": "undefined"
      },
      "comments": {
        "type": "string",
        "title": "General Comments"
      },
      "projectSummaryReportPayment": {
        "title": "Project Summary Report Payment",
        "type": "number"
      },
      "paymentNotes": {
        "title": "Notes for the Payment",
        "type": "string"
      },
      "dateSentToCsnr": {
        "title": "Date Invoice Sent to CSNR",
        "type": "string",
        "default": "undefined"
      }
    }
  }
}
