{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Project Summary Report",
    "required": ["reportDueDate"],
    "properties": {
      "reportDueDate": {
        "type": "string",
        "title": "Report Due Date"
      },
      "submittedDate": {
        "type": ["null", "string"],
        "title": "Received Date",
        "default": "undefined"
      },
      "comments": {
        "type": ["null", "string"],
        "title": "General Comments",
        "default": "undefined"
      },
      "projectSummaryReportPayment": {
        "title": "Project Summary Report Payment",
        "type": ["null", "number"],
        "default": "undefined"
      },
      "paymentNotes": {
        "title": "Notes for the Payment",
        "type": ["null", "string"],
        "default": "undefined"
      },
      "dateSentToCsnr": {
        "title": "Date Invoice Sent to CSNR",
        "type": ["null", "string"],
        "default": "undefined"
      }
    }
  }
}
