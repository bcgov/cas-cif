{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Reporting Requirement",
    "required": ["reportDueDate"],
    "properties": {
      "reportDueDate": {
        "type": "string",
        "title": "Report Due Date"
      },
      "submittedDate": {
        "type": ["null", "string"],
        "title": "Report Received Date",
        "default": null
      },
      "comments": {
        "type": ["null", "string"],
        "title": "General Comments",
        "default": null
      }
    }
  }
}
