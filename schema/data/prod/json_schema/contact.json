{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": ["givenName", "familyName", "email", "phone"],
    "properties": {
      "givenName": {
        "type": "string",
        "title": "Given Name"
      },
      "familyName": {
        "type": "string",
        "title": "Family Name"
      },
      "email": {
        "type": "string",
        "title": "Email"
      },
      "phone": {
        "type": "string",
        "title": "Phone"
      },
      "phoneExt": {
        "type": "string",
        "title": "Phone Extension"
      },
      "companyName": {
        "type": "string",
        "title": "Company Name"
      },
      "contactPosition": {
        "type": "string",
        "title": "Position"
      },
      "comments": {
        "type": "string",
        "title": "Comments"
      }
    }
  }
}
